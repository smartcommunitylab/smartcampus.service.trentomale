<?xml version="1.0" encoding="UTF-8"?>
<service-definition xmlns="http://www.example.org/interface-schema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.example.org/interface-schema ../interface-schema.xsd ">
	<interface serviceURI="smartcampus.service.TrentoMale">
		
		<operation name="TimeTableTrentoMale" storable="true">
			<parameter name="stationsList" type="String" />
			<output type="smartcampus.service.trentomale.data.message.Trentomale.Station" repeated="true" />
		</operation>
		<operation name="TrainsTrentoMale" storable="true">
			<output type="smartcampus.service.trentomale.data.message.Trentomale.Train" repeated="true" />
		</operation>		
		
	</interface>

	<implementation package="smartcampus.service.trentomale">
		<operationImpl name="TimeTableTrentoMale" />
		<operationImpl name="TrainsTrentoMale" />
	</implementation>
	
</service-definition>


